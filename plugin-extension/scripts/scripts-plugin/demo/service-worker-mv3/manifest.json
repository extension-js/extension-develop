{
  "manifest_version": 3,
  "name": "Service Worker MV3",
  "version": "1.0.0",
  "background": {
    "service_worker": "./dist/background.background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["/dist/*.txt", "/dist/*.json", "/dist/*.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}
